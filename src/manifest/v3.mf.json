{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "mobius",
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": ["index.js"],

      "matches": [
        "https://*.mobius.cloud/qbeditor/*",
        "https://*.mobius.cloud/*/addquestion",
        "https://*.blank.page/",
        "*://localhost/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
}
